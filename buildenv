export ZOPEN_STABLE_URL="https://github.com/stigtsp/moreutils.git"
export ZOPEN_STABLE_DEPS="zoslib make coreutils getopt"
export ZOPEN_CONFIGURE="skip"
export ZOPEN_BUILD_LINE="STABLE"
export ZOPEN_MAKE_OPTS="sponge parallel errno pee mispipe"
export ZOPEN_INSTALL_OPTS="PREFIX=\$ZOPEN_INSTALL_DIR install"

zopen_check_results()
{
  dir="$1"
  pfx="$2"
  chk="$1/$2_check.log"

  # Echo the following information to gauge build health
  echo "actualFailures:0"
  echo "totalTests:1"
  echo "expectedFailures:0"
  echo "expectedTotalTests:1"
}

zopen_get_version()
{
  # Modify to echo the version of your tool/library
  # Rather than hardcoding the version, obtain the version by running the tool/library
  echo "1.0.0"
}
